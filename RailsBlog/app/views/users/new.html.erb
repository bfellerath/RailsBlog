<h4>be a new user</h4>



<%= form_for @user do |f| %>
<div class="row">

        <div class="six columns">
    <%= f.label :username %>
    <%= f.text_field :username, :class => 'u-full-width'%>
    </div>
    <div class="six columns">
    <%= f.label :password %>
    <%= f.password_field :password, :class => 'u-full-width'%>
</div>
</div>

    <%= f.submit :id => 'dark-button3' %>
<% end %>
